<LinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:background="#fff"
	android:orientation="vertical"
	tools:context="com.iec.dwx.timer.Fragments.AchievementFragment">

	<android.support.design.widget.AppBarLayout
		android:layout_width="match_parent"
		android:layout_height="wrap_content">

		<android.support.v7.widget.Toolbar
			android:id="@+id/toolbar_achievement"
			android:layout_width="match_parent"
			app:navigationIcon="@drawable/ic_arrow_back_black_24dp"
			android:layout_height="?actionBarSize"
			app:subtitleTextColor="@color/toolbar_text_color"
			app:layout_scrollFlags="scroll|enterAlways"
			app:subtitle="@string/menu_time_achievement"/>
	</android.support.design.widget.AppBarLayout>

	<android.support.v7.widget.RecyclerView
		android:id="@+id/rv_achievement"
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		>


	</android.support.v7.widget.RecyclerView>
</LinearLayout>
